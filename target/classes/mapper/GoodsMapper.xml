<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mhys.mapper.GoodsMapper">
    <select id="findAllGoods" resultType="com.mhys.model.Goods">
      select * from goods
  </select>
    <delete id="delByidGoods" parameterType="java.lang.Integer">
        delete  from user where id=#{goods_id}
    </delete>

</mapper>